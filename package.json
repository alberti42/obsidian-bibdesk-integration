{
    "name": "bibtex-integration",
    "version": "1.0.0",
    "description": "Allows importing bibtex entries.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "grammar": "peggy --format es src/grammar.pegjs -o src/peggy.mjs",
        "bookmark_resolver": "swiftc -o dist/bookmark_resolver bookmark_resolver/bookmark_resolver.swift"
    },
    "keywords": [],
    "author": "Andrea Alberti",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/node": "^16.18.105",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "obsidian": "latest",
        "peggy": "^4.0.3",
        "rollup": "^2.79.1",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-web-worker-loader": "^1.6.1",
        "tslib": "2.4.0",
        "typescript": "4.7.4"
    },
    "dependencies": {
        "@rollup/plugin-replace": "^5.0.7",
        "bplist-parser": "^0.3.2",
        "chokidar": "^3.6.0",
        "monkey-around": "^3.0.0",
        "promise-worker": "^2.0.1"
    }
}
