{
    "name": "bibdesk-integration",
    "version": "1.1.0",
    "description": "Import BibTex citations into your notes and open PDF documents linked in a BibDesk library.",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json bookmark_resolver/bookmark_resolver.swift",
        "grammar": "peggy --format es src/grammar.pegjs -o src/peggy.mjs",
        "bookmark_resolver": "node esbuild.bookmark_resolver.mjs"
    },
    "keywords": [],
    "author": "Andrea Alberti",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.18.105",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.25.5",
        "esbuild-plugin-copy": "^2.1.1",
        "obsidian": "latest",
        "peggy": "^4.0.3",
        "tslib": "^2.8.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "add": "^2.0.6",
        "bplist-parser": "^0.3.2",
        "chokidar": "^3.6.0",
        "monkey-around": "^3.0.0"
    }
}
